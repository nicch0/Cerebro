{
    "name": "cerebro",
    "version": "0.1.0",
    "description": "Obsidian-first, AI-powered second brain that thinks with you.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint lib/ --max-warnings=0",
        "lint:fix": "eslint lib/ --fix",
        "fix-all": "pnpm run format:fix && pnpm run lint:fix",
        "test": "jest --config jest.config.js",
        "test:watch": "jest --config jest.config.js --watch",
        "test:coverage": "jest --config jest.config.js --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:all": "pnpm run test && pnpm run test:e2e"
    },
    "keywords": [],
    "author": "Nick Chong",
    "peerDependencies": {
        "obsidian": "^1.1.0"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^1.1.17",
        "@ai-sdk/deepseek": "^0.1.15",
        "@ai-sdk/google": "^1.1.25",
        "@ai-sdk/openai": "^1.2.5",
        "@ai-sdk/xai": "^1.1.15",
        "@anthropic-ai/sdk": "^0.39.0",
        "@codemirror/state": "^6.5.2",
        "@codemirror/view": "^6.36.4",
        "ai": "^4.1.62",
        "obsidian": "^1.8.7",
        "openai": "^4.87.4",
        "pino": "^9.6.0",
        "sse": "github:mpetazzoni/sse.js"
    },
    "devDependencies": {
        "@playwright/test": "^1.51.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.10",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "builtin-modules": "^5.0.0",
        "electron": "^35.0.2",
        "esbuild": "^0.25.1",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-import-resolver-typescript": "^4.2.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.11.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.6",
        "tslib": "^2.8.1",
        "typescript": "^5.8.2"
    },
    "pnpm": {
        "ignoredBuiltDependencies": [
            "electron"
        ],
        "onlyBuiltDependencies": [
            "electron"
        ]
    }
}
