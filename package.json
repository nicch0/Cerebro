{
    "name": "cerebro",
    "version": "0.1.0",
    "description": "Obsidian-first, AI-powered second brain that thinks with you.",
    "type": "module",
    "main": "main.js",
    "scripts": {
        "dev": "pnpm run --parallel '/^dev:.*/'",
        "dev:tailwind": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --watch",
        "dev:vite": "vite build --mode development --watch",
        "build": "pnpm run '/^build:.*/'",
        "build:tailwind": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --minify",
        "build:vite": "tsc -noEmit -skipLibCheck && vite build --mode production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint src/ --max-warnings=0",
        "lint:fix": "eslint src/ --fix",
        "fix-all": "pnpm run format:fix && pnpm run lint:fix",
        "svelte-check": "svelte-check --tsconfig tsconfig.json"
    },
    "keywords": [],
    "author": "Nick Chong",
    "peerDependencies": {
        "obsidian": "^1.1.0"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.25.1",
        "@lucide/svelte": "^0.503.0",
        "@playwright/test": "^1.52.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.14.1",
        "@typescript-eslint/eslint-plugin": "^8.31.0",
        "@typescript-eslint/parser": "^8.31.0",
        "autoprefixer": "^10.4.21",
        "bits-ui": "^1.3.19",
        "builtin-modules": "^5.0.0",
        "clsx": "^2.1.1",
        "electron": "^35.2.0",
        "eslint": "^9.25.1",
        "eslint-config-prettier": "^10.1.2",
        "eslint-import-resolver-typescript": "^4.3.4",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.11.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "rimraf": "^6.0.1",
        "svelte": "^5.28.1",
        "svelte-check": "^4.1.6",
        "svelte-preprocess": "^6.0.3",
        "tailwind-merge": "^3.2.0",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "ts-jest": "^29.3.2",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.0",
        "vite": "^6.3.2"
    },
    "pnpm": {
        "ignoredBuiltDependencies": [
            "electron"
        ],
        "onlyBuiltDependencies": [
            "electron"
        ]
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^1.2.10",
        "@ai-sdk/deepseek": "^0.2.12",
        "@ai-sdk/google": "^1.2.13",
        "@ai-sdk/openai": "^1.3.17",
        "@ai-sdk/xai": "^1.2.14",
        "@codemirror/language": "^6.11.0",
        "@codemirror/state": "^6.5.2",
        "@codemirror/view": "^6.36.5",
        "ai": "^4.3.9",
        "marked": "^15.0.9",
        "obsidian": "^1.8.7",
        "pino": "^9.6.0",
        "sse": "github:mpetazzoni/sse.js",
        "svelte-radix": "^2.0.1",
        "tippy": "^0.0.0"
    }
}
